{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "builds": [
    {
      "src": "dist/server/vercel-handler.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/public/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "dist/server/vercel-handler.js" }
  ]
}